#include <iostream>
using std::cout;
using std::endl;
class A {
   public:
     ~A() {
        cout << "A" << endl;
      }
};
class B : public A{
   public :
      ~B() {
        cout << "B" << endl;
      }
};
int main (int argc, char **argv) {
   A *ptr = new B();
   delete ptr;
   return 0;
}
