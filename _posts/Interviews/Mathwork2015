#include <iostream>
using std::cout;
using std::endl;
int main (int argc, char **argv) {
  int *arr = new int[5];
  for (int idx = 0; idx < 5; ++idx) {
    arr[idx] = idx;
        cout << *(idx + arr) * 2 <<" ";
    }
  cout<<endl;
  delete arr;
  return 0;
}

